{
    "builds": [
        {
            "src": "app/api/**/*.ts",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "node_modules/playwright/**/*"
                ]
            }
        }
    ],
    "functions": {
        "app/api/**/*.ts": {
            "maxDuration": 60
        }
    }
}